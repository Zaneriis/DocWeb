                  <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description_2">
                    <lang xmlns="http://www.w3.org/XML/1998/namespace#">en</lang>
                    <titre>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description_2/titre">
                        <rdf:value>Numerical Linear Algebra for engineers</rdf:value>
                      </rdf:Description>
                    </titre>
                    <rdf:_1>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description_2/titre"/>
                    </rdf:_1>
                    <langue>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description_2/langue">
