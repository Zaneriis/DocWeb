                  <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_23/description_2">
                    <lang xmlns="http://www.w3.org/XML/1998/namespace#">en</lang>
                    <titre>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_23/description_2/titre">
                        <rdf:value>Graph theory and operational research</rdf:value>
                      </rdf:Description>
                    </titre>
                    <rdf:_1>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_23/description_2/titre"/>
                    </rdf:_1>
                    <langue>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_23/description_2/langue">
