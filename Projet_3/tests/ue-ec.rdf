                              </rdf:Description>
                            </li>
                            <rdf:_3>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_2/description/objectif/ul/li_3"/>
                            </rdf:_3>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_2/description/objectif/ul/li_4">
