                  <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2">
                    <lang xmlns="http://www.w3.org/XML/1998/namespace#">en</lang>
                    <titre>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/titre">
                        <rdf:value>Image Processing</rdf:value>
                      </rdf:Description>
                    </titre>
                    <rdf:_1>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/titre"/>
                    </rdf:_1>
                    <langue>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/langue">
                        <rdf:value>French</rdf:value>
                      </rdf:Description>
                    </langue>
                    <rdf:_2>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/langue"/>
                    </rdf:_2>
                    <objectif>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/objectif">
                        <p>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/objectif/p">
                            <rdf:value>The aim of this course is to give the basics, principals, and skills that can be used in image processing for the following applications: segmentation, analysis, compression...</rdf:value>
                          </rdf:Description>
                        </p>
                      </rdf:Description>
                    </objectif>
                    <rdf:_3>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/objectif"/>
                    </rdf:_3>
                    <programme>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme">
                        <ul>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul">
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li">
                                <rdf:value>Binary image processing</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_1>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li"/>
                            </rdf:_1>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li_2">
                                <rdf:value>Image quality improvement</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_2>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li_2"/>
                            </rdf:_2>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li_3">
                                <rdf:value>Image compression</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_3>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li_3"/>
                            </rdf:_3>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li_4">
                                <rdf:value>Image segmentation</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_4>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li_4"/>
                            </rdf:_4>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li_5">
                                <rdf:value>Texture characterization</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_5>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li_5"/>
                            </rdf:_5>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_22/description_2/programme/ul/li_6">
                                <rdf:value>Region representation</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_6>
