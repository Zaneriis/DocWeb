                  <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_15/description_2">
                    <lang xmlns="http://www.w3.org/XML/1998/namespace#">en</lang>
                    <titre>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_15/description_2/titre">
