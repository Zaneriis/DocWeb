                    <titre>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description/titre">
                        <rdf:value>Maîtrise des Grands Projets Informatiques</rdf:value>
                      </rdf:Description>
                    </titre>
                    <rdf:_1>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description/titre"/>
                    </rdf:_1>
