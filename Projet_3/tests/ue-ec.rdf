                                <rdf:value>Organisational schema of a company
	      </rdf:value>
                                <ul>
                                  <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description_2/programme/ul/li/ul">
                                    <li>
                                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description_2/programme/ul/li/ul/li">
                                        <rdf:value>basic concepts : direction, fonctional services, matrix organisation</rdf:value>
                                      </rdf:Description>
                                    </li>
                                    <rdf:_1>
                                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description_2/programme/ul/li/ul/li"/>
                                    </rdf:_1>
                                    <li>
                                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description_2/programme/ul/li/ul/li_2">
                                        <rdf:value>human ressources management</rdf:value>
                                      </rdf:Description>
                                    </li>
                                    <rdf:_2>
                                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description_2/programme/ul/li/ul/li_2"/>
                                    </rdf:_2>
                                  </rdf:Description>
                                </ul>
                              </rdf:Description>
                            </li>
                            <rdf:_1>
