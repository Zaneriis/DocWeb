                        <rdf:_18>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_49/description/evaluation/ul_5"/>
                        </rdf:_18>
                        <p>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_49/description/evaluation/p_14">
