                  <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2">
                    <lang xmlns="http://www.w3.org/XML/1998/namespace#">en</lang>
                    <titre>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/titre">
                        <rdf:value>Information theory</rdf:value>
                      </rdf:Description>
                    </titre>
                    <rdf:_1>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/titre"/>
                    </rdf:_1>
                    <langue>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/langue">
                        <rdf:value>French</rdf:value>
                      </rdf:Description>
                    </langue>
                    <rdf:_2>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/langue"/>
                    </rdf:_2>
                    <objectif>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/objectif">
                        <p>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/objectif/p">
                            <rdf:value>Introduction to the field of information theory and its applications to the communication theory.
	    Presentation of the basic quantities for entropy and mutual information. Presentation of the basic
	    algorithms for source coding, channel coding and cryptography.</rdf:value>
                          </rdf:Description>
                        </p>
                      </rdf:Description>
                    </objectif>
                    <rdf:_3>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/objectif"/>
                    </rdf:_3>
                    <programme>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme">
                        <p>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/p">
                            <rdf:value>Mathematical tools :</rdf:value>
                          </rdf:Description>
                        </p>
                        <rdf:_1>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/p"/>
                        </rdf:_1>
                        <ul>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul">
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul/li">
                                <rdf:value>Probabilities, entropy and mutual information,</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_1>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul/li"/>
                            </rdf:_1>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul/li_2">
                                <rdf:value>Entropy rate of stochastic processes,</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_2>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul/li_2"/>
                            </rdf:_2>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul/li_3">
                                <rdf:value>Rate distortion theory,</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_3>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul/li_3"/>
                            </rdf:_3>
