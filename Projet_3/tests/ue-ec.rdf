                              </rdf:Description>
                            </li>
                            <rdf:_3>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_15/description_2/programme/ul_3/li_3"/>
                            </rdf:_3>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_15/description_2/programme/ul_3/li_4">
                                <rdf:value>Rejection of a hypothesis with parametric and non parametric testing methods</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_4>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_15/description_2/programme/ul_3/li_4"/>
                            </rdf:_4>
                          </rdf:Description>
                        </ul>
                        <rdf:_8>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_15/description_2/programme/ul_3"/>
                        </rdf:_8>
                      </rdf:Description>
                    </programme>
