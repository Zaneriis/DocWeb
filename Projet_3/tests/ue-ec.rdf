                    <lang xmlns="http://www.w3.org/XML/1998/namespace#">fr</lang>
                    <titre>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/titre">
                        <rdf:value>Méthodes numériques pour l'ingénieur</rdf:value>
                      </rdf:Description>
                    </titre>
                    <rdf:_1>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/titre"/>
                    </rdf:_1>
