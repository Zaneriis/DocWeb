                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_49/description/evaluation/ul_6/li_3"/>
                            </rdf:_3>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_49/description/evaluation/ul_6/li_4">
