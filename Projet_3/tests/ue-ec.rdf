                    <rdf:_2>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/langue"/>
                    </rdf:_2>
                    <objectif>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/objectif">
                        <ul>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/objectif/ul">
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/objectif/ul/li">
                                <rdf:value> Acquérir des compétences générales en analyse numérique matricielle </rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_1>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/objectif/ul/li"/>
                            </rdf:_1>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/objectif/ul/li_2">
                                <rdf:value> Acquérir les principales méthodes numériques de résolution des systèmes d'équations linéaires </rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_2>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/objectif/ul/li_2"/>
                            </rdf:_2>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/objectif/ul/li_3">
                                <rdf:value> Mise en oeuvre à l'aide de logiciels comme Matlab, Octave ou Python </rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_3>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/objectif/ul/li_3"/>
                            </rdf:_3>
                          </rdf:Description>
                        </ul>
                        <xs:comment xmlns:xs="http://www.w3.org/TR/2008/REC-xml-20081126#">&lt;p&gt;Le but de cet EC est d'initier les étudiants aux problèmes liés à l'utilisation des méthodes numériques de résolution des problèmes de l'ingénieur, en insistant sur leur expérimentation à l'aide d'un logiciel comme Matlab ou Scilab.&lt;/p&gt; </xs:comment>
                      </rdf:Description>
                    </objectif>
                    <rdf:_3>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/objectif"/>
                    </rdf:_3>
                    <programme>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme">
                        <ul>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul">
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li">
                                <rdf:value>Introduction au calcul matriciel : formulation d'un problème sous forme matricielle, opérations vectorielles et matricielles </rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_1>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li"/>
                            </rdf:_1>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li_2">
                                <rdf:value> Factorisations de matrices (LU, Cholesky, QR) </rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_2>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li_2"/>
                            </rdf:_2>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li_3">
                                <rdf:value> Applications à la résolution numérique de systèmes linéaires et aux problèmes des moindres carrés </rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_3>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li_3"/>
                            </rdf:_3>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li_4">
                                <rdf:value> Calcul de valeurs propres et singulières avec application à l'étude du conditionnement d'une matrice, notion de pré-conditionnement </rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_4>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li_4"/>
                            </rdf:_4>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li_5">
                                <rdf:value> Initiation aux méthodes itératives d'optimisation</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_5>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_6/description/programme/ul/li_5"/>
                            </rdf:_5>
