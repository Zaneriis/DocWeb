                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_2/description/objectif/ul/li">
                                <rdf:value> Savoir réaliser un diagramme de conception de base de données (e.g. E-A)</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_1>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_2/description/objectif/ul/li"/>
                            </rdf:_1>
