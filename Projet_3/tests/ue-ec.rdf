                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_2/description/objectif/ul/li_2">
                                <rdf:value> Savoir passer de ce modèle de très haut niveau à un modèle relationnel</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_2>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_2/description/objectif/ul/li_2"/>
