                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li">
                                <rdf:value>Channel capacity,</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_1>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li"/>
                            </rdf:_1>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li_2">
                                <rdf:value>Error detection and/or correction algorithms,</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_2>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li_2"/>
                            </rdf:_2>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li_3">
                                <rdf:value>Hamming codes,</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_3>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li_3"/>
                            </rdf:_3>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li_4">
                                <rdf:value>Binary cyclic codes and BCH codes</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_4>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li_4"/>
                            </rdf:_4>
                            <li>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li_5">
                                <rdf:value>Binary convolutional codes</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_5>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_21/description_2/programme/ul_3/li_5"/>
