                    <objectif>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description_2/objectif">
                        <p>
                          <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description_2/objectif/p">
                            <rdf:value>The ability of managing large projects in which the computer science part is important (and becoming more and more important) Preparing student to the PIC by writting the Quality Plan and Configurations description.</rdf:value>
                          </rdf:Description>
                        </p>
                      </rdf:Description>
                    </objectif>
                    <rdf:_3>
                      <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_26/description_2/objectif"/>
                    </rdf:_3>
