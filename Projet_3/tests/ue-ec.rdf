                                <rdf:value>des grands comptes</rdf:value>
                              </rdf:Description>
                            </li>
                            <rdf:_3>
                              <rdf:Description rdf:about="#contratsDEtudes/ecs/ec_49/description/programme/ul/li_3"/>
                            </rdf:_3>
                          </rdf:Description>
                        </ul>
                        <rdf:_2>
